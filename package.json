{
    "name": "taro3",
    "version": "1.0.0",
    "private": true,
    "description": "taro3",
    "templateInfo": {
        "name": "taro-ui",
        "typescript": true,
        "css": "stylus"
    },
    "scripts": {
        "build:weapp": "taro build --type weapp",
        "build:swan": "taro build --type swan",
        "build:alipay": "taro build --type alipay",
        "build:tt": "taro build --type tt",
        "build:h5": "taro build --type h5",
        "build:rn": "taro build --type rn",
        "build:qq": "taro build --type qq",
        "build:jd": "taro build --type jd",
        "build:quickapp": "taro build --type quickapp",
        "dev:weapp": "npm run build:weapp -- --watch",
        "dev:swan": "npm run build:swan -- --watch",
        "dev:alipay": "npm run build:alipay -- --watch",
        "dev:tt": "npm run build:tt -- --watch",
        "dev:h5": "npm run build:h5 -- --watch",
        "dev:rn": "npm run build:rn -- --watch",
        "dev:qq": "npm run build:qq -- --watch",
        "dev:jd": "npm run build:jd -- --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch",
        "icon": "npx iconfont-taro",
        "start": "cross-env ENV=development NODE_ENV=development npm run dev:weapp",
        "sp": "cross-env ENV=production NODE_ENV=development npm run dev:weapp",
        "sk": "cross-env ENV=KAYOU NODE_ENV=development npm run dev:weapp",
        "sl": "cross-env ENV=LIANGYOU NODE_ENV=development npm run dev:weapp",
        "bu": "cross-env ENV=development NODE_ENV=production npm run build:weapp",
        "indie": "cross-env ENV=development NODE_ENV=production taro build --type weapp --blended",
        "bp": "cross-env ENV=production NODE_ENV=production npm run build:weapp",
        "br": "cross-env ENV=REALLY NODE_ENV=production npm run build:weapp",
        "bk": "cross-env ENV=KAYOU NODE_ENV=production npm run build:weapp",
        "ba": "cross-env ENV=ANLIFANG NODE_ENV=production npm run build:weapp",
        "bs": "cross-env ENV=SANX NODE_ENV=production npm run build:weapp",
        "bl": "cross-env ENV=LIANGYOU NODE_ENV=production npm run build:weapp"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "",
    "dependencies": {
        "@babel/runtime": "7.17.9",
        "@miniprogram-component-plus/sticky": "^1.0.1",
        "@tarojs/cli": "3.4.7",
        "@tarojs/components": "3.4.7",
        "@tarojs/extend": "3.4.7",
        "@tarojs/plugin-framework-react": "3.4.7",
        "@tarojs/redux": "2.2.10",
        "@tarojs/redux-h5": "2.2.10",
        "@tarojs/rn-runner": "3.4.7",
        "@tarojs/runtime": "3.4.7",
        "@tarojs/taro": "3.4.10",
        "@tarojs/taro-rn": "3.4.7",
        "cross-env": "7.0.3",
        "fs-copy-file-sync": "^1.1.1",
        "fs-extra": "^11.1.1",
        "html2canvas": "^1.4.1",
        "lodash": "4.17.15",
        "md5": "^2.3.0",
        "mini-code": "^1.0.1",
        "nervjs": "^1.5.7",
        "npm-tarojs-utils": "^1.0.2",
        "number-precision": "^1.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.0",
        "react-native": "^0.67.4",
        "react-redux": "7.2.8",
        "redux": "4.2.0",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.2.2",
        "redux-thunk": "^2.3.0",
        "sa-sdk-miniprogram": "^1.20.5",
        "sha1": "^1.1.1",
        "spark-md5": "^3.0.2",
        "taro-code": "^3.5.2",
        "taro3-code": "^2.2.0",
        "wxbarcode": "^1.0.2",
        "wxmp-qrcode": "^2.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@babel/plugin-transform-typescript": "^7.10.5",
        "@tarojs/mini-runner": "^3.2.0",
        "@tarojs/plugin-indie": "0.0.7",
        "@tarojs/webpack-runner": "^3.2.0",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^17.0.4",
        "@types/webpack-env": "^1.13.6",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "babel-preset-taro": "^3.2.0",
        "eslint": "^6.8.0",
        "eslint-config-taro": "^3.2.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-react": "^7.8.2",
        "eslint-plugin-react-hooks": "^1.6.1",
        "stylelint": "9.3.0",
        "taro-iconfont-cli": "^3.3.0",
        "typescript": "^4.1.0"
    }
}
